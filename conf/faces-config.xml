    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <description>
      The backing bean for the user preferences, with session scope
    </description>
    <managed-bean-name>userPreferencesBean</managed-bean-name>
    <managed-bean-class>org.freebxml.omar.client.ui.thin.UserPreferencesBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
