      <property-name>userPreferencesBean</property-name>
      <value>#{userPreferencesBean}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <description>
      The backing bean for the query objects
    </description>
    <managed-bean-name>query</managed-bean-name>
    <managed-bean-class>org.freebxml.omar.client.ui.thin.QueryBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <description>
      The backing bean for the search results objects
    </description>
    <managed-bean-name>model</managed-bean-name>
