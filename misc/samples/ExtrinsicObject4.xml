<?xml version = "1.0" encoding = "UTF-8"?>
<!--$Header: /cvsroot/ebxmlrr/omar/misc/samples/ExtrinsicObject4.xml,v 1.1.1.1 2003/09/24 03:40:35 farrukh_najmi Exp $-->
<AdhocQueryRequest xmlns = "urn:oasis:names:tc:ebxml-regrep:query:xsd:2.0" xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation = "urn:oasis:names:tc:ebxml-regrep:query:xsd:2.0 file:///C:/open/ebxmlrr-spec/misc/schema/query.xsd">
	<ResponseOption returnType = "RegistryEntry" returnComposedObjects = "false"/>
	<FilterQuery>
		<ExtrinsicObjectQuery>
			<ExtrinsicObjectFilter>                    
				<Clause>               
					<CompoundClause connectivePredicate = "And">
						<Clause>
							<SimpleClause leftArgument = "isOpaque">
								<BooleanClause booleanPredicate = "false"/>
							</SimpleClause>
						</Clause>
						<Clause>
	               				<CompoundClause connectivePredicate = "Or">
								<Clause>
									<SimpleClause leftArgument = "isOpaque">
										<BooleanClause booleanPredicate = "true"/>
									</SimpleClause>
								</Clause>
								<Clause>
									<SimpleClause leftArgument = "majorVersion">
										<RationalClause logicalPredicate = "EQ">
											<IntClause>1</IntClause>
										</RationalClause>
									</SimpleClause>
								</Clause>
	           					</CompoundClause>
						</Clause>                       
					</CompoundClause>                    
				</Clause>
			</ExtrinsicObjectFilter>
		</ExtrinsicObjectQuery>
	</FilterQuery>
</AdhocQueryRequest>
