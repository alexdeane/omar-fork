<!--$Header: /cvsroot/ebxmlrr/omar/misc/samples/Organization3.xml,v 1.1.1.1 2003/09/24 03:41:23 farrukh_najmi Exp $-->
<AdhocQueryRequest xmlns = "urn:oasis:names:tc:ebxml-regrep:query:xsd:2.0" xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation = "urn:oasis:names:tc:ebxml-regrep:query:xsd:2.0 query.xsd">
	<ResponseOption returnType = "LeafClass" returnComposedObjects = "true"/>
	<FilterQuery>
		<OrganizationQuery>
			<PostalAddressFilter>
				<Clause>
					<SimpleClause leftArgument = "country">
						<StringClause stringPredicate = "-Equal">FRANCE</StringClause>
					</SimpleClause>
				</Clause>
			</PostalAddressFilter>
		</OrganizationQuery>
	</FilterQuery>
</AdhocQueryRequest>
